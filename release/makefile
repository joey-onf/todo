# -*- makefile -*-
## -----------------------------------------------------------------------
## -----------------------------------------------------------------------

.DEFAULT_GOAL: view

## -----------------------------------------------------------------------
## -----------------------------------------------------------------------
all:

## -----------------------------------------------------------------------
## -----------------------------------------------------------------------
view ::
	pandoc README.md | lynx --stdin

## -----------------------------------------------------------------------
## -----------------------------------------------------------------------
help ::
	@printf 'Usage: $(MAKE) [options] [target] ...\n'
	@printf '  %-33.33s %s\n' 'view' \
	  'Regenerate and load README.md for interactive viewing'

# [EOF]
